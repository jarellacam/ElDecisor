{
  "version": 2,
  "functions": {
    "backend/app/main.py": {
      "runtime": "vercel-python@0.0.6"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "backend/app/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/index.html"
    }
  ]
}